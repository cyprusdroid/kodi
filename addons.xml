<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.video.cyprusdroid" name=".CyprusDroid Wizard" version="1.0.0" provider-name="CyprusDroid">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video executable</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">CyprusDroid Wizard</summary>
    <description lang="en">Best XBMC installation with CyprusDroid Wizard</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="repository.cyprusdroid" name=".CyprusDroid_Repository" version="1.0.0" provider-name="CyprusDroid">
	<extension point="xbmc.addon.repository" name=".CyprusDroid_Repository">
		<info compressed="false">https://github.com/cyprusdroid/kodi/blob/master/addons.xml</info>
		<checksum>https://github.com/cyprusdroid/kodi/blob/master/addons.xml.md5</checksum>
		<datadir zip="true">https://github.com/cyprusdroid/kodi/blob/master/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Testing.</summary>
		<description>Testing.</description>
		<platform>all</platform>
	</extension>
</addon>
</addons>
